[core]
    editor = vim
    excludesfile = ~/.gitignore_global
    autocrlf = input
[color]
    branch = auto
    diff = auto
    status = auto
[user]
    name = 
    email = 
[alias]
    b = branch
    cm = commit
    co = checkout
    cob = checkout -b
    cp = cherry-pick
    d = diff
    dwd = diff --word-diff
    f = fetch
    gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
    mg = merge
    st = status
    sw = switch
[ghq]
    root = ~/dev
    root = ~/dev/src